# AIKEN v2 - Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Drive API (Cloud Run backend - US region for free tier)
VITE_DRIVE_API_URL=https://your-cloud-run-service.run.app

# AI Generator API (Cloud Run backend - US region for free tier)
VITE_AI_API_URL=https://your-ai-generator-service.run.app
